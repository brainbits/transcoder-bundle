    "homepage": "http://brainbits.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregor Welters",
            "email": "gwelters@brainbits.net"
        },
        {
            "name": "Phillip Look",
            "email": "plook@brainbits.net"
